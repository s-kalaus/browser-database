{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": [true, {
        "multiline": "never",
        "singleline": "never"
      }],
    "member-access": [true, "no-public"],
    "ordered-imports": false
  },
  "rulesDirectory": []
}